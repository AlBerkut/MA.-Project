{
    "name": "ma-project-api",
    "main": "src/app.ts",
    "scripts": {
        "dev": "nodemon src/app.ts",
        "lint": "npx eslint . --ignore-path .gitignore --ext .js,.ts --fix",
        "test:dev": "jest --no-cache --watch --passWithNoTests",
        "test": "jest --no-cache --silent --passWithNoTests",
        "build": "tsc && npm run copyTranslations && npm run copyTemplates",
        "prestart": "npm run build",
        "start": "node dist/src/app.js",
        "loadPlaces": "npx ts-node src/loadAndParseRestPlaces",
        "prepareDB": "npx ts-node ./src/migrations/prepareDB",
        "copyTranslations": "cp -r ./src/translations/ ./dist/src/translations/",
        "copyTemplates": "cp -r ./src/templates/ ./dist/src/templates/"
    },
    "author": "Dmitry Kit",
    "devDependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/bluebird": "^3.5.30",
        "@types/body-parser": "^1.19.0",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.6",
        "@types/ejs": "^3.0.1",
        "@types/express": "^4.17.2",
        "@types/jsonwebtoken": "^8.3.8",
        "@types/node": "^13.7.7",
        "@types/nodemailer": "^6.4.0",
        "@types/swagger-jsdoc": "^3.0.2",
        "@types/swagger-ui-express": "^4.1.1",
        "@types/validator": "^12.0.1",
        "@typescript-eslint/eslint-plugin": "^2.25.0",
        "@typescript-eslint/parser": "^2.25.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.19.0",
        "husky": "^3.0.9",
        "jest": "^24.9.0",
        "nodemon": "^2.0.2",
        "ts-node": "^8.6.2",
        "typescript": "^3.8.3"
    },
    "dependencies": {
        "@curveball/http-errors": "^0.3.0",
        "@googlemaps/google-maps-services-js": "^2.4.2",
        "axios": "^0.19.2",
        "bcrypt": "^4.0.1",
        "body-parser": "^1.19.0",
        "cookie-parser": "^1.4.4",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "ejs": "^3.0.1",
        "express": "^4.17.1",
        "express-async-errors": "^3.1.1",
        "google-auth-library": "^5.10.1",
        "http-custom-errors": "^8.0.0",
        "jsonwebtoken": "^8.5.1",
        "log4js": "^6.2.1",
        "mysql2": "^2.1.0",
        "nodemailer": "^6.4.6",
        "reflect-metadata": "^0.1.13",
        "sequelize": "^5.21.5",
        "sequelize-cli": "^5.5.1",
        "sequelize-typescript": "^1.1.0",
        "swagger-jsdoc": "^3.5.0",
        "swagger-ui-express": "^4.1.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint && npm run test && cd .. && git add ."
        }
    }
}
